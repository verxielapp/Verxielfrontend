# Ignore files that might contain secrets
*.env
*.env.local
*.env.development
*.env.production
.env*

# Ignore build artifacts that might contain environment variables
build/static/js/*.map
build/static/css/*.map
build/service-worker.js.map

# Ignore node modules
node_modules/

# Ignore git files
.git/
.gitignore

# Ignore any other sensitive files
*.log
*.tmp
*.temp 